<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag5"
    android:background="@color/white"
    tools:context="Fragments.Fragment_5">

    <!-- TODO: Update blank fragment layout -->
    <include layout="@layout/s_back"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:layout_width="250dp"
            android:layout_height="200dp"
            android:id="@+id/frag5_logo"
            android:layout_marginRight="50dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/logo"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="The Multi mode Protector"
            android:id="@+id/frag5_logotxt"
            android:textColor="@color/red"
            android:textStyle="italic"
            android:layout_below="@+id/frag5_logo"
            android:layout_alignLeft="@+id/frag5_logo"
            android:layout_alignStart="@+id/frag5_logo" />



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="CRIT benefits:"
            android:textColor="@color/red"
            android:id="@+id/frag5_head"
            android:textSize="30sp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="34dp"
            android:layout_marginStart="34dp"
            android:layout_below="@+id/frag5_img1"
            android:layout_alignLeft="@+id/frag5_pt1"
            android:layout_alignStart="@+id/frag5_pt1" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/frag5_img1"
            android:src="@drawable/frag5_img1"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="20dp" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/frag5_buimg1"
            android:layout_alignLeft="@+id/frag5_img1"
            android:layout_alignStart="@+id/frag5_img1"
            android:layout_marginLeft="71dp"
            android:src="@drawable/bullet_img"
            android:layout_alignBottom="@+id/frag5_pt1"
            android:layout_alignTop="@+id/frag5_pt1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Converts fat soluble substance to water soluble"
            android:id="@+id/frag5_pt1"
            android:textSize="26sp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/frag5_head"
            android:layout_toRightOf="@+id/frag5_buimg1"
            android:layout_toEndOf="@+id/frag5_buimg1" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/frag5_buimg2"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/frag5_buimg1"
            android:src="@drawable/bullet_img"
            android:layout_alignRight="@+id/frag5_buimg1"
            android:layout_alignEnd="@+id/frag5_buimg1"
            android:layout_alignLeft="@+id/frag5_buimg1"
            android:layout_alignStart="@+id/frag5_buimg1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Reduces particle size at the absorption point"
            android:id="@+id/frag5_pt2"
            android:textSize="26sp"
            android:layout_alignTop="@+id/frag5_buimg2"
            android:layout_alignLeft="@+id/frag5_pt1"
            android:layout_alignStart="@+id/frag5_pt1" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/frag5_buimg3"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/frag5_pt2"
            android:src="@drawable/bullet_img"
            android:layout_alignRight="@+id/frag5_buimg2"
            android:layout_alignEnd="@+id/frag5_buimg2"
            android:layout_alignLeft="@+id/frag5_buimg2"
            android:layout_alignStart="@+id/frag5_buimg2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Increases the stability of sensitive ingredients by protecting from oxidation"
            android:id="@+id/frag5_pt3"
            android:textSize="26sp"
            android:layout_alignTop="@+id/frag5_buimg3"
            android:layout_alignLeft="@+id/frag5_pt2"
            android:layout_alignStart="@+id/frag5_pt2"
            android:layout_alignRight="@+id/frag5_logo"
            android:layout_alignEnd="@+id/frag5_logo" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/frag5_buimg4"
            android:src="@drawable/bullet_img"
            android:layout_alignRight="@+id/frag5_buimg3"
            android:layout_alignEnd="@+id/frag5_buimg3"
            android:layout_alignLeft="@+id/frag5_buimg2"
            android:layout_alignStart="@+id/frag5_buimg2"
            android:layout_alignBottom="@+id/frag5_pt4"
            android:layout_alignTop="@+id/frag5_pt4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Translates erratic absorption to smooth"
            android:id="@+id/frag5_pt4"
            android:textSize="26sp"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/frag5_pt3"
            android:layout_alignLeft="@+id/frag5_pt3"
            android:layout_alignStart="@+id/frag5_pt3" />

    </RelativeLayout>

</FrameLayout>
