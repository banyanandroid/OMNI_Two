<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag12"
    android:background="@color/white"
    tools:context="com.banyan.omni_two.Fragments.Fragment_12">

    <include layout="@layout/preg_back_change"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView24"
            android:layout_marginLeft="74dp"
            android:layout_marginStart="74dp"
            android:src="@drawable/frag12_img1"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:id="@+id/imageView25"
            android:padding="2dp"
            android:layout_toRightOf="@+id/imageView24"
            android:layout_toEndOf="@+id/imageView24"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/bullet_img"
            android:layout_alignTop="@+id/imageView24" />

        <TextView
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Effective anti-inflammatory/antioxidant"
            android:id="@+id/textView14"
            android:layout_marginBottom="-20dp"
            android:textColor="@color/red"
            android:layout_above="@+id/imageView25"
            android:layout_toRightOf="@+id/imageView25"
            android:layout_toEndOf="@+id/imageView25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Prevents common cold"
            android:id="@+id/textView15"
            android:layout_marginLeft="5dp"
            android:textColor="@color/black"
            android:layout_alignTop="@+id/imageView25"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="May of use in preventing PIH/Preclampsia due to its ability to curb inflammation"
            android:id="@+id/textView16"
            android:layout_marginTop="5dp"
            android:textColor="@color/black"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/textView15"
            android:layout_alignLeft="@+id/textView15"
            android:layout_alignStart="@+id/textView15"
            android:layout_alignRight="@+id/textView17"
            android:layout_alignEnd="@+id/textView17" />

        <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:id="@+id/imageView26"
            android:padding="2dp"
            android:layout_below="@+id/textView16"
            android:layout_alignLeft="@+id/imageView25"
            android:layout_alignStart="@+id/imageView25"
            android:layout_marginTop="15dp"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Effectively controls Motion Sickness"
            android:id="@+id/textView17"
            android:textColor="@color/red"
            android:layout_alignTop="@+id/imageView26"
            android:layout_alignLeft="@+id/textView15"
            android:layout_alignStart="@+id/textView15" />

        <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:id="@+id/imageView27"
            android:padding="2dp"
            android:layout_marginTop="14dp"
            android:src="@drawable/bullet_img"
            android:layout_below="@+id/textView17"
            android:layout_alignLeft="@+id/imageView26"
            android:layout_alignStart="@+id/imageView26" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Regulate Blood glucose"
            android:id="@+id/textView18"
            android:textColor="@color/red"
            android:layout_alignTop="@+id/imageView27"
            android:layout_alignLeft="@+id/textView17"
            android:layout_alignStart="@+id/textView17" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Helps in controlling and preventing gestation diabetes"
            android:id="@+id/textView19"
            android:textColor="@color/black"
            android:layout_marginRight="130dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/textView18"
            android:layout_alignLeft="@+id/textView16"
            android:layout_alignStart="@+id/textView16" />
    </RelativeLayout>

</FrameLayout>
