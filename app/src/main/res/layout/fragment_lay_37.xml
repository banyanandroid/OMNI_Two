<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag37"
    android:background="@color/white"
    tools:context="com.banyan.omni_two.Fragments.Fragment_13">

    <include layout="@layout/preg_back_change"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="left|center_vertical">


        <ImageView
            android:layout_width="200dp"
            android:layout_height="70dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Ginger and its compound"
            android:id="@+id/frag38_img"
            android:textColor="@color/black"
            android:src="@drawable/frag37_img"
            android:layout_marginLeft="71dp"
            android:layout_marginStart="71dp"
            android:layout_marginTop="72dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/frag37_bul1"
            android:layout_marginTop="3dp"
            android:paddingTop="3dp"
            android:layout_below="@+id/frag38_img"
            android:layout_alignLeft="@+id/frag38_img"
            android:layout_alignStart="@+id/frag38_img"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Dehydrozingerone in Ginger a documented radioprotector “When administrated through I.P. or oral route, its compound was effective in protecting against gamma radiation-induced sickness &amp; mortality”"
            android:id="@+id/frag42_pt1_txt"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:layout_marginRight="240dp"
            android:layout_alignTop="@+id/frag37_bul1"
            android:layout_toRightOf="@+id/frag37_bul1"
            android:layout_toEndOf="@+id/frag37_bul1" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:paddingTop="3dp"
            android:layout_marginTop="8dp"
            android:id="@+id/frag37_bul2"
            android:layout_below="@+id/frag42_pt1_txt"
            android:layout_alignLeft="@+id/frag37_bul1"
            android:layout_alignStart="@+id/frag37_bul1"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Zingerone in Ginger exert antiapoptotic, antigenotoxic, and antiinflammatory activity due to inhibition of radiation induced oxidative stress and free radicals scavenging potential."
            android:id="@+id/frag42_pt2_txt"
            android:layout_marginRight="85dp"
            android:textColor="@color/black"
            android:layout_alignTop="@+id/frag37_bul2"
            android:layout_alignLeft="@+id/frag42_pt1_txt"
            android:layout_alignStart="@+id/frag42_pt1_txt" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/frag37_bul3"
            android:paddingTop="3dp"
            android:layout_marginTop="8dp"
            android:layout_below="@+id/frag42_pt2_txt"
            android:layout_alignLeft="@+id/frag37_bul2"
            android:layout_alignStart="@+id/frag37_bul2"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Zingerol selectively protects normal tissues against the Radiation, apart from offering radioprotection effect."
            android:id="@+id/frag42_pt3_txt"
            android:textColor="@color/black"
            android:layout_marginRight="50dp"
            android:layout_alignTop="@+id/frag37_bul3"
            android:layout_alignLeft="@+id/frag42_pt2_txt"
            android:layout_alignStart="@+id/frag42_pt2_txt" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/frag37_bul4"
            android:paddingTop="3dp"
            android:layout_marginTop="8dp"
            android:layout_below="@+id/frag42_pt3_txt"
            android:layout_alignLeft="@+id/frag37_bul3"
            android:layout_alignStart="@+id/frag37_bul3"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Pretreatment also decreases oxidative stress induced UB B and activated caspase-3, 8,9 &amp; Fas expression"
            android:id="@+id/frag38_pt4_txt"
            android:layout_marginRight="150dp"
            android:textColor="@color/black"
            android:layout_alignTop="@+id/frag37_bul4"
            android:layout_alignLeft="@+id/frag42_pt3_txt"
            android:layout_alignStart="@+id/frag42_pt3_txt" />

    </RelativeLayout>

</FrameLayout>
