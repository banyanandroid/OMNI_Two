<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag9"
    android:background="@color/white"
    tools:context="Fragments.Fragment_9">

    <include layout="@layout/preg_back_change"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:layout_width="660dp"
            android:layout_height="2px"
            android:background="@color/black"
            android:layout_marginLeft="87dp"
            android:layout_marginTop="110dp"
            android:id="@+id/view"
            android:layout_marginRight="200dp"
            android:layout_marginStart="57dp">
        </View>

        <TextView
            android:layout_width="520dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="The American College of Obstetricians and Gynecologists (ACOG) states that -"
            android:id="@+id/frag9_pt1"
            android:textStyle="bold"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:textColor="@color/red"
            android:layout_below="@+id/view"
            android:layout_alignLeft="@+id/view"
            android:layout_alignStart="@+id/view" />

        <TextView
            android:layout_width="520dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="“Treatment of nausea &amp; vomiting of pregnancy with ginger has shown beneficial effects and can be considered as a nonpharmacologic option.”"
            android:id="@+id/frag9_pt2"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:layout_marginRight="210dp"
            android:layout_below="@+id/frag9_pt1"
            android:layout_alignLeft="@+id/frag9_pt1"
            android:layout_alignStart="@+id/frag9_pt1" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/black"
            android:id="@+id/view2"
            android:layout_marginTop="10dp"
            android:layout_marginRight="90dp"
            android:layout_below="@+id/frag9_pt2"
            android:layout_alignLeft="@+id/frag9_pt2"
            android:layout_alignStart="@+id/frag9_pt2">
        </View>

        <ImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:id="@+id/imageView14"
            android:src="@drawable/frag8_img1"
            android:layout_toRightOf="@+id/frag9_pt1"
            android:layout_toEndOf="@+id/frag9_pt1"
            android:layout_below="@+id/view"
            android:layout_above="@+id/frag9_pt3" />

        <ImageView
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:id="@+id/imageView15"
            android:layout_below="@+id/view2"
            android:layout_alignLeft="@+id/view2"
            android:layout_alignStart="@+id/view2"
            android:src="@drawable/frag9_img2"
            android:layout_above="@+id/textView3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="The U.K. National Health Service (NHS) through the National Institute for Health and Clinical Excellence [NICE], has included ginger in its list of acceptable therapies for the treatment of nausea and vomiting during early pregnancy."
            android:id="@+id/frag9_pt3"
            android:layout_below="@+id/view2"
            android:layout_toRightOf="@+id/imageView15"
            android:layout_marginTop="10dp"
            android:textColor="@color/black"
            android:layout_marginRight="80dp"
            android:layout_marginLeft="10dp"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="@color/black"
            android:id="@+id/view3"
            android:layout_marginLeft="77dp"
            android:layout_marginRight="80dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/frag9_pt3"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true">
        </View>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:gravity="center"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Ginger is USFDA GRAS certified"
            android:id="@+id/textView3"
            android:textStyle="bold"
            android:textColor="@color/red"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/view3"
            android:layout_alignLeft="@+id/frag9_pt3"
            android:layout_alignStart="@+id/frag9_pt3" />

        <ImageView
            android:layout_width="150dp"
            android:layout_height="70dp"
            android:id="@+id/imageView16"
            android:layout_alignTop="@+id/textView3"
            android:layout_toRightOf="@+id/textView3"
            android:layout_toEndOf="@+id/textView3"
            android:layout_marginLeft="39dp"
            android:layout_marginStart="39dp"
            android:src="@drawable/frag9_img3" />


    </RelativeLayout>

</FrameLayout>
